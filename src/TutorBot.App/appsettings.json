{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server={host};Database={database};User Id={login};Password={password};Timeout=15;"
  },
  "TelegramService": {
    "Token": "{token}",
    "AdminKey": "{key}",
    "Enable": true,
    "GroupNumbers": [
      "РИ-000000",
      "test",
      "1"
    ]
  },
  "GigaChat": {
    "SecretKey": "{Token}",
    "IsCommercial": false,
    "IgnoreTLS": false
  },
  "YandexSearch": {
    "IAMToken": "{Token}",
    "FolderId": "{Token}"
  }
}
